<h1>Make a new list</h1>


<%= simple_form_for(@list) do |f| %>
    <div class="field">
      <%= f.input :name, label: "list name" %>
      <%= f.input :photo, as: :file %>
    </div>

    <div class="actions">
      <%= f.submit "Create List" %>
    </div>
<% end %>

<%= link_to "Return to Index", lists_path %>
